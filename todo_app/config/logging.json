{
  "version": 1,
  "filters": {
  },
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(threadName)s - %(name)s - [%(module)s.%(funcName)s:%(lineno)d] ] - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filters": [
        
      ],
      "stream": "ext://sys.stdout"
    },
    "info_file_handler": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename": "todo_appi.log",
      "filters": [
        
      ],
      "maxBytes": 10485760,
      "backupCount": 20,
      "encoding": "utf8"
    }
  },
  "loggers": {
    "urllib3.connectionpool": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler"
      ],
      "propagate": false
    },
    "pika": {
      "level": "WARN",
      "handlers": [
        "console",
        "info_file_handler"
      ],
      "propagate": false
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console",
      "info_file_handler"
    ]
  }
}
